CREATE TABLE HIGHDEGREE(
STUDENTID INT NOT NULL,
PROFESSORID INT  NOT NULL,
PROJECTID INT  NOT NULL,
SCHOOLNAME VARCHAR(32),
INSTITUTIONNAME VARCHAR(32),
HIGHDEGREE VARCHAR(10),
	PRIMARY KEY (STUDENTID, PROFESSORID, PROJECTID),
	FOREIGN KEY (SCHOOLNAME, INSTITUTIONNAME) REFERENCES SCHOOL (NAME, INSTITUTIONNAME),
	FOREIGN KEY (STUDENTID) REFERENCES RESEARCHER(ID),
	FOREIGN KEY (PROJECTID) REFERENCES PROJECT(ID)

);