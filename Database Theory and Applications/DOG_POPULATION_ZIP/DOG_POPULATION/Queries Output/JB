CREATE TABLE POSITION(
RESEARCHERID INT NOT NULL,
STARTDATE INT NOT NULL,
SCHOOLNAME VARCHAR(32) NOT NULL,
INSTITUTIONNAME VARCHAR(32) NOT NULL,
ENDDATE INT,S RESEARCHER (ID),
FOREIGN KEY (SCHOOLNAME, INSTITUTIONNA
PRIMARY KEY (RESEARCHERID, STARTDATE, SCHOOLNAME, INSTITUTIONNAME),
FOREIGN KEY (RESEARCHERID) REFERENCEME) REFERENCES SCHOOL (NAME, INSTITUTIONNAME)
);